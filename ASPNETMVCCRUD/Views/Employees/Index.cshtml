@{
}
@model IList<ASPNETMVCCRUD.Models.Domain.Employee>
<h1>
    Employees
</h1>
<table class="table">
    <thead><tr><th>Id</th><th>Name</th><th>Email</th><th>Salary</th><th>DateofBirth</th><th>Department</th></tr></thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr><td>@item.Id</td><td>@item.Name</td><td>@item.Email</td><td>@item.Salary</td><td>@item.DateOfBirth.ToString("dd-MM-yyyy")</td><td>@item.Department</td><td>@*<a href="Employees/View/@item.Id">View</a>*@  @Html.ActionLink("Display","View","Employees",new {sid=@item.Id}) </td></tr>
        }
    </tbody>
</table>